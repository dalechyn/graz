{
  "name": "@graz/connectors",
  "description": "A collection of connectors for graz",
  "version": "0.0.2",
  "repository": "https://github.com/graz-sh/graz.git",
  "homepage": "https://github.com/graz-sh/graz",
  "bugs": "https://github.com/graz-sh/graz/issues",
  "exports": {
    "./coin98": {
      "types": "./dist/coin98.d.ts",
      "default": "./dist/coin98.js"
    },
    "./compass": {
      "types": "./dist/compass.d.ts",
      "default": "./dist/compass.js"
    },
    "./cosmostation": {
      "types": "./dist/cosmostation.d.ts",
      "default": "./dist/cosmostation.js"
    },
    "./keplr": {
      "types": "./dist/keplr.d.ts",
      "default": "./dist/keplr.js"
    },
    "./metamaskLeap": {
      "types": "./dist/metamaskLeap.d.ts",
      "default": "./dist/metamaskLeap.js"
    },
    "./vectis": {
      "types": "./dist/vectis.d.ts",
      "default": "./dist/vectis.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "graz": ">=0.0.37"
  },
  "devDependencies": {
    "graz": "*",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "graz",
    "graz-connectors",
    "@graz/connectors"
  ],
  "license": "MIT",
  "dependencies": {
    "@vectis/extension-client": "^0.7.2"
  }
}
